{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src/",
        "esModuleInterop": true,
        "module": "CommonJS",
        "moduleDetection": "force",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "target": "ES2020"
    }
}